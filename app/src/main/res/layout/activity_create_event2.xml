<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/neutralbakgrundnogubbe"
    tools:context="com.sportify.createEvent.createEventPageFive.createEventActivity">


    <TextView
        android:id="@+id/profileName"
        android:layout_width="62dp"
        android:layout_height="34dp"
        android:text="@string/event_host"
        android:textSize="20sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="2dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.026"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.273"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/clickNext"
        android:layout_width="138dp"
        android:layout_height="47dp"
        android:text="@string/event_next"
        android:textSize="20sp"
        android:onClick="onClick"
        android:clickable="true"
        android:layout_below="@+id/profileName"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/clickBack"
        android:layout_width="113dp"
        android:layout_height="43dp"
        android:text="@string/event_back"
        android:textSize="20sp"
        android:onClick="onClick"
        android:clickable="true"
        android:layout_marginEnd="13dp"
        android:layout_alignBaseline="@+id/clickNext"
        android:layout_alignBottom="@+id/clickNext"
        android:layout_toStartOf="@+id/clickNext"
        android:layout_marginRight="13dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginStart="8dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="237dp"
        android:layout_height="179dp"
        app:srcCompat="@drawable/addpeople"
        android:layout_alignParentEnd="true"
        android:background="@drawable/border"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/clickNext"
        android:layout_marginStart="57dp"
        android:layout_marginTop="212dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="213dp"
        app:layout_constraintLeft_toLeftOf="@+id/profileName"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription" />


</android.support.constraint.ConstraintLayout>
